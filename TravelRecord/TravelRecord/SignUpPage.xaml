<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TravelRecord.SignUpPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="Name" Placeholder="Name" />
            <Entry x:Name="Email" Placeholder="Email" Keyboard="Email"/>
            <Entry x:Name="Password" Placeholder="Password" IsPassword="True"/>
            <Entry x:Name="UserName" Placeholder="UserName" />
            <Button x:Name="SaveButton" Text="Save" Clicked="SaveButton_OnClicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>